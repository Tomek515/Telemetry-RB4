{
  "version": 4,
  "configurations": [
    {
      "name": "MSYS2 UCRT64",
      "compilerPath": "C:/msys64/ucrt64/bin/g++.exe",
      "intelliSenseMode": "windows-gcc-x64",
      "cStandard": "c17",
      "cppStandard": "c++17",

      "includePath": [
        "${workspaceFolder}/include",
        "${workspaceFolder}/src"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/include",
          "${workspaceFolder}/src"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ]
}
